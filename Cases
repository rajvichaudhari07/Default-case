#include <stdio.h>
    int main() {
        int num = 12;
        switch (num) {
            case 7:
                printf("Value is 14");
                break;
            case 8:
                printf("Value is 8");
                break;
            case 9:
                printf("Value is 9");
                break;
            default:
                printf("Out of range");
                break;
        }
        return 0;
    }
